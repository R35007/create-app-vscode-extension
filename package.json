{
  "name": "create-app",
  "displayName": "Create App",
  "description": "Generates an interactive form and quick pick for any CLI to Create an APP.",
  "version": "6.0.0",
  "publisher": "Thinker",
  "license": "MIT",
  "icon": "media/images/ca-logo.png",
  "author": {
    "name": "Sivaraman",
    "email": "sendmsg2siva@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/R35007/create-app-vscode-extension"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "keywords": [
    "Create App",
    "Create Project",
    "Angular",
    "Django",
    "Electron",
    "Express",
    "Gatsby",
    "Ionic",
    "NestJs",
    "NextJs",
    "Preact",
    "React",
    "React Native",
    "Svelte",
    "Vite",
    "Vs Code Extension",
    "Vue"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "create-app.interactive",
        "title": "Interactive",
        "category": "Create App"
      },
      {
        "command": "create-app.quick",
        "title": "Quick",
        "category": "Create App"
      }
    ],
    "configuration": {
      "title": "Create App",
      "properties": {
        "create-app.settings.quick.promptType": {
          "type": "string",
          "enum": [
            "Default",
            "Required",
            "None"
          ],
          "enumDescriptions": [
            "Prompt field config that required or prompt flag is set to true",
            "Prompt only required field config",
            "Don't prompt any field configs"
          ],
          "default": "Default",
          "description": "Set the Create App: Quick command prompt type."
        },
        "create-app.settings.quick.promptCommandString": {
          "type": "boolean",
          "default": true,
          "description": "Prompt for command string change in quick command pallet before executing in terminal."
        },
        "create-app.settings.quick.promptExecutionPath": {
          "type": "boolean",
          "default": true,
          "description": "If false it always executes in the active workspace folder location."
        },
        "create-app.settings.interactive.showAppIcons": {
          "type": "boolean",
          "default": true,
          "description": "Set to false to hide the app icons. Hiding icons will improve the rendering performance."
        },
        "create-app.settings.interactive.groupApps": {
          "type": "boolean",
          "default": true,
          "description": "Set to false to list all the apps in aside without group names"
        },
        "create-app.settings.hiddenApps": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "Prove list of app names to hide"
        },
        "create-app.settings.hiddenGroups": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "Prove list of group names to hide"
        },
        "create-app.settings.customAppPaths": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "Set custom file or folder paths of the custom app json. please restart the vscode to take effect."
        },
        "create-app.settings.customApps": {
          "type": "array",
          "description": "Provide the list of custom app details. please restart the vscode to take effect.",
          "items": {
            "type": "object",
            "properties": {
              "appName": {
                "type": "string"
              },
              "groupNames": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "relatedAppNames": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "commandTemplate": {
                "anyOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                ]
              },
              "hide": {
                "type": "boolean"
              },
              "fields": {
                "type": "object",
                "additionalProperties": {
                  "anyOf": [
                    {
                      "type": "object",
                      "additionalProperties": false,
                      "properties": {
                        "label": {
                          "type": "string"
                        },
                        "order": {
                          "type": "number"
                        },
                        "prefix": {
                          "type": "string"
                        },
                        "suffix": {
                          "type": "string"
                        },
                        "value": {
                          "type": [
                            "string",
                            "boolean"
                          ]
                        },
                        "description": {
                          "type": "string"
                        },
                        "prompt": {
                          "type": "boolean"
                        },
                        "hide": {
                          "type": "boolean"
                        },
                        "type": {
                          "type": "string",
                          "const": "textbox"
                        },
                        "placeholder": {
                          "type": "string"
                        },
                        "pattern": {
                          "type": "string"
                        },
                        "required": {
                          "type": "boolean"
                        },
                        "errors": {
                          "type": "object",
                          "properties": {
                            "required": {
                              "type": "string"
                            },
                            "pattern": {
                              "type": "string"
                            }
                          },
                          "additionalProperties": false
                        }
                      },
                      "required": [
                        "type"
                      ]
                    },
                    {
                      "type": "object",
                      "additionalProperties": false,
                      "properties": {
                        "label": {
                          "type": "string"
                        },
                        "order": {
                          "type": "number"
                        },
                        "prefix": {
                          "type": "string"
                        },
                        "suffix": {
                          "type": "string"
                        },
                        "value": {
                          "type": [
                            "string",
                            "boolean"
                          ]
                        },
                        "description": {
                          "type": "string"
                        },
                        "prompt": {
                          "type": "boolean"
                        },
                        "hide": {
                          "type": "boolean"
                        },
                        "type": {
                          "type": "string",
                          "enum": [
                            "radio",
                            "dropdown"
                          ]
                        },
                        "required": {
                          "type": "boolean"
                        },
                        "errors": {
                          "type": "object",
                          "properties": {
                            "required": {
                              "type": "string"
                            }
                          },
                          "additionalProperties": false
                        },
                        "options": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "label": {
                                "type": "string"
                              },
                              "value": {}
                            },
                            "required": [
                              "label"
                            ],
                            "additionalProperties": false
                          }
                        }
                      },
                      "required": [
                        "options",
                        "type"
                      ]
                    },
                    {
                      "type": "object",
                      "additionalProperties": false,
                      "properties": {
                        "label": {
                          "type": "string"
                        },
                        "order": {
                          "type": "number"
                        },
                        "prefix": {
                          "type": "string"
                        },
                        "suffix": {
                          "type": "string"
                        },
                        "value": {
                          "type": [
                            "string",
                            "boolean"
                          ]
                        },
                        "description": {
                          "type": "string"
                        },
                        "prompt": {
                          "type": "boolean"
                        },
                        "hide": {
                          "type": "boolean"
                        },
                        "type": {
                          "type": "string",
                          "const": "checkbox"
                        },
                        "checkedValue": {
                          "type": [
                            "string",
                            "boolean"
                          ]
                        },
                        "unCheckedValue": {
                          "type": [
                            "string",
                            "boolean"
                          ]
                        },
                        "checkedLabel": {
                          "type": "string"
                        },
                        "unCheckedLabel": {
                          "type": "string"
                        },
                        "required": {
                          "type": "boolean"
                        },
                        "errors": {
                          "type": "object",
                          "properties": {
                            "required": {
                              "type": "string"
                            }
                          },
                          "additionalProperties": false
                        }
                      },
                      "required": [
                        "type"
                      ]
                    },
                    {
                      "type": "object",
                      "additionalProperties": false,
                      "properties": {
                        "label": {
                          "type": "string"
                        },
                        "order": {
                          "type": "number"
                        },
                        "prefix": {
                          "type": "string"
                        },
                        "suffix": {
                          "type": "string"
                        },
                        "value": {
                          "type": [
                            "string",
                            "boolean"
                          ]
                        },
                        "description": {
                          "type": "string"
                        },
                        "prompt": {
                          "type": "boolean"
                        },
                        "hide": {
                          "type": "boolean"
                        },
                        "type": {
                          "type": "string",
                          "const": "browse"
                        },
                        "placeholder": {
                          "type": "string"
                        },
                        "buttonText": {
                          "type": "string"
                        },
                        "required": {
                          "type": "boolean"
                        },
                        "canSelectFile": {
                          "type": "boolean"
                        },
                        "canSelectFolder": {
                          "type": "boolean"
                        },
                        "pattern": {
                          "type": "string"
                        },
                        "errors": {
                          "type": "object",
                          "properties": {
                            "required": {
                              "type": "string"
                            },
                            "pattern": {
                              "type": "string"
                            }
                          },
                          "additionalProperties": false
                        }
                      },
                      "required": [
                        "type"
                      ]
                    }
                  ]
                }
              },
              "description": {
                "type": "string"
              },
              "logoPath": {
                "type": "string"
              },
              "prerequisites": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "label": {
                      "type": "string"
                    },
                    "description": {
                      "type": "string"
                    },
                    "command": {
                      "type": "string"
                    },
                    "href": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "label"
                  ],
                  "additionalProperties": false
                }
              },
              "additionalCommands": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "label": {
                      "type": "string"
                    },
                    "description": {
                      "type": "string"
                    },
                    "command": {
                      "type": "string"
                    },
                    "href": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "label"
                  ],
                  "additionalProperties": false
                }
              },
              "resources": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "label": {
                      "type": "string"
                    },
                    "description": {
                      "type": "string"
                    },
                    "command": {
                      "type": "string"
                    },
                    "href": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "label"
                  ],
                  "additionalProperties": false
                }
              },
              "tags": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "order": {
                "type": "number"
              }
            },
            "required": [
              "appName"
            ],
            "additionalProperties": false
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/json-schema": "^7.0.15",
    "@types/node": "14.x",
    "@types/vscode": "^1.50.0",
    "@types/webpack": "^5.28.0",
    "@vscode/webview-ui-toolkit": "^0.8.5",
    "ajv": "^8.17.1",
    "bootstrap": "^5.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.8.0",
    "prettier": "3.3.3",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  }
}
